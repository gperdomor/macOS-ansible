"*":
  "atom-beautify":
    html:
      beautify_on_save: true
  core:
    projectHome: "/Users/gperdomor/Developer"
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
        ".flowconfig"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "tree-view"
    ]
    telemetryConsent: "no"
    titleBar: "custom"
  editor:
    fontSize: 13
  "exception-reporting":
